{
	"name": "scorm-player",
	"version": "1.1.0",
	"description": "SCORM 2004 player, unpacker and manifest parser for Node.js / Next.js with storage adapters and React iframe renderer.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"scorm",
		"scorm2004",
		"scorm player",
		"nextjs",
		"react",
		"elearning",
		"lms",
		"imsmanifest"
	],
	"author": "Shmelev Sergei",
	"license": "MIT",
	"dependencies": {
		"@supabase/supabase-js": "^2.9.0",
		"jszip": "^3.10.1",
		"xmldom": "^0.6.0"
	},
	"peerDependencies": {
		"react": ">=18 <21",
		"react-dom": ">=18 <21"
	},
	"devDependencies": {
		"@types/react": "^18.3.27",
		"@types/react-dom": "^18.3.7",
		"@types/xmldom": "^0.1.34",
		"tsup": "^7.3.0",
		"typescript": "^5.4.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shmelevsergei/scorm-player-2004.git"
	},
	"bugs": {
		"url": "https://github.com/shmelevsergei/scorm-player-2004/issues"
	},
	"homepage": "https://github.com/shmelevsergei/scorm-player-2004#readme"
}
